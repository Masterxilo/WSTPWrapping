WSPutList(wstype, what, len) {WSPut##wstype##List(stdlink, what, len);}\n\
#define WSPut(wstype, what) {WSPut##wstype(stdlink, what);}\n\n\n#include \
<string.h>\n#include <memory.h>\nvoid freemallocstrcpy(char** dest, const \
char* const src) {\n    if (*dest) memoryFree(*dest);\n    auto sz = \
strlen(src) + 1;\n    memoryAllocate(*dest, sz);\n    strcpy(*dest, src);\n}\n\
